                <![CDATA[capital6 = #{memUserInfo.capital6},]]>
            </if>
            <if test="memUserInfo.capital7 != null and memUserInfo.capital7 >0">
                <![CDATA[capital7 = #{memUserInfo.capital7},]]>
            </if>
            <if test="memUserInfo.capital8 != null and memUserInfo.capital8 >0">
                <![CDATA[capital8 = #{memUserInfo.capital8},]]>
            </if>
        </set>
        <where>
            <![CDATA[AND pk_id = #{userId, jdbcType=BIGINT}]]>
        </where>
    </update>

    <!--注册人数-->
    <select id="selectRegisterNum" parameterType="Map" resultMap="MemUserInfo">
        <![CDATA[
		SELECT
		 *
		FROM
			t_mem_user_info t
		]]>
        <where>
            <if test="where.beginDate != null">
                <![CDATA[AND t.create_time >= #{where.beginDate, jdbcType=TIMESTAMP}]]>
            </if>
            <if test="where.endDate != null">
                <![CDATA[AND t.create_time <= #{where.endDate, jdbcType=TIMESTAMP}]]>
            </if>
        </where>
    </select>
    <!--更新用户积分-->
    <update id="updateUserIntegral" parameterType="Map">
