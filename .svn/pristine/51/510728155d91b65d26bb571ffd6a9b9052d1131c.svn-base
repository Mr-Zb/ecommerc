package com.fun.framework.web.dto;

import java.util.Date;

public class QueryDto extends BaseDto {
	private static final long serialVersionUID = 4811559199409088658L;
	private String keyword;
	private Integer page = 1;
	private Integer rows = 10;
	private Date beginDate;
	private Date endDate;
	private String sort;
	private String order;

	public QueryDto() {
	}

	public QueryDto(String keyword, Integer page, Integer rows, Date beginDate, Date endDate, String sort,
			String order) {
		this();
		this.keyword = keyword;
		this.page = page;
		this.rows = rows;
		this.beginDate = beginDate;
		this.endDate = endDate;
		this.sort = sort;
		this.order = order;
	}

	public String getKeyword() {
		return this.keyword;
	}

	public void setKeyword(String keyword) {
		this.keyword = keyword;
	}

	public Integer getPage() {
		return this.page;
	}

	public void setPage(Integer page) {
		this.page = page;
	}

	public Integer getRows() {
		return this.rows;
	}

	public void setRows(Integer rows) {
		this.rows = rows;
	}

	public Date getBeginDate() {
		return this.beginDate;
	}

	public void setBeginDate(Date beginDate) {
		this.beginDate = beginDate;
	}

	public Date getEndDate() {
		return this.endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public String getSort() {
		return this.sort;
	}

	public void setSort(String sort) {
		this.sort = sort;
	}

	public String getOrder() {
		return this.order;
	}

	public void setOrder(String order) {
		this.order = order;
	}
}